@model IEnumerable<AppDocumentosSharePoint.Models.PreguntaRespuestaCanonica>

@{
    ViewData["Title"] = "Preguntas y Respuestas Can√≥nicas";
}

<h2>@ViewData["Title"]</h2>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Tema</th>
            <th>Pregunta</th>
            <th>Respuesta</th>
            <th>Fuente</th>
            <th>Fecha</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Tema</td>
                <td>@item.PreguntaCanonica</td>
                <td>@item.RespuestaOficial</td>
                <td>@item.FuenteOficial</td>
                <td>@item.FechaRegistro?.ToString("dd/MM/yyyy")</td>
            </tr>
        }
    </tbody>
</table>
